ASL=*.asl *.asx
AMI=*.sdl *.ssp
BIOS2=*.cif *.lst *.ini
Make=*.mak Makefile
C=*.c *.h
C++=*.cpp *.hpp *.cxx *.hxx *.cc <C>
VHDL=*.hdl *.vhd *.vhdl *.vbe *.vst
Web=*.htm *.html *.js

